{
    "name": "javascript-action",
    "version": "1.0.0",
    "description": "JavaScript Action Template",
    "main": "index.js",
    "scripts": {
      "lint": "eslint .",
      "test": "jest marketplace.spec.js --ci --reporters=default --reporters=jest-junit",
      "all": "npm run lint && npm run prepare && npm run test"
    },
    "jest-junit": {
      "outputDirectory": "reports",
